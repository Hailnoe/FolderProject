{
  "manifest_version": 2,

  "name": "Folder for Links!",
  "description": "An extension that creates folders to store and organize hyperlinks",
  "version": "0.1",

  "browser_action": {
   "default_popup": "popup.html"
  },
  "permissions": [
    "notifications", "tabs", "history", "storage"
   ],
   "background": {
    "scripts": ["Javascript/popup.js", "Javascript/background.js"],
    "persistent": false
  }
}
